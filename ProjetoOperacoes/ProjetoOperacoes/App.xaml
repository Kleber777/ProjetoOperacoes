<Application x:Class="ProjetoOperacoes.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vwPrincipal="clr-namespace:ProjetoOperacoes.View"
             xmlns:vmPrincipal="clr-namespace:ProjetoOperacoes.ViewModel"
             xmlns:vwTestePage="clr-namespace:ProjetoOperacoes.View"
             xmlns:vmTestePage="clr-namespace:ProjetoOperacoes.ViewModel">

    <!--StartupUri="MainWindow.xaml"-->
    <Application.Resources>
        <DataTemplate DataType="{x:Type vmPrincipal:PrincipalViewModel}">
            <vwPrincipal:PrincipalView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmTestePage:TestePageViewModel}">
            <vwTestePage:TestePage/>
        </DataTemplate>
    </Application.Resources>
</Application>
